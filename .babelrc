{
  "presets": ["env", "react","stage-0"],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": true,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    ["import", {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
    }]
  ]
}